<p id="notice"><%= notice %></p>

<h1>user_products</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_products.each do |user_product| %>
      <tr>
        <td><%= link_to 'Show', user_product %></td>
        <td><%= link_to 'Edit', edit_user_product_path(user_product) %></td>
        <td><%= link_to 'Destroy', user_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New user_product', new_user_product_path %>
